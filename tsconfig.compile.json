  "compilerOptions": {
    "target": "ES2018",
    "module": "CommonJS",
    "moduleResolution": "node",
    "skipLibCheck": true,
    "importHelpers": true,
    "strict": true,
    "experimentalDecorators": true,
    "resolveJsonModule": true,
    "noImplicitOverride": true,
    "esModuleInterop": true
  }
}